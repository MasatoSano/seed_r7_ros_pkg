<?xml version="1.0"?>
<launch>
  <!-- dummy scan -->
  <arg name="scan_topic" default="map"/>
  <node name="dummy_scan" pkg="noid_mover_navigation" type="dummy_scan.py">
    <remap from="map" to="$(arg scan_topic)"/>
  </node>

  <!-- Run map navigation -->
  <arg name="map_file" default="$(find noid_mover_navigation)/maps/dummy.yaml"/>
  <arg name="costmap_file" default="$(find noid_mover_navigation)/maps/dummy.yaml"/>
  <include file="$(find noid_mover_navigation)/launch/static_map_navigation.launch">
    <arg name="map_file" value="$(arg map_file)" />
    <arg name="costmap_file" value="$(arg costmap_file)"/>
  </include>

</launch>
